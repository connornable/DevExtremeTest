    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@n-able/apex-schematics": "^1.0.6",
    "devextreme": "^22.1.3",
    "devextreme-angular": "^22.1.3",
    "devextreme-aspnet-data-nojquery": "2.8.6",
